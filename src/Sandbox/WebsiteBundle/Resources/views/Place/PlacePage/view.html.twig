{% extends 'SandboxWebsiteBundle:Layout:layout.html.twig' %}
{% block content %}
    <div class="container">
        <h1>{{ page.title }}</h1>
        {% include '@SandboxWebsite/SeoModule/top.html.twig' %}
        {{ render_pageparts(page, 'main') }}

        {% if places is defined and places is not null %}
            {% for place in places %}
                <h3>{{ place.title }}</h3>
                <div>{{ place.summary }}</div>
            {% endfor %}
        {% endif %}
    </div>
{% include '@SandboxWebsite/SeoModule/bottom.html.twig' %}
    {#{% include 'KunstmaanArticleBundle:AbstractArticlePage:view.html.twig' %}#}
{% endblock %}
{% block seoFooter %}
    {% include "@SandboxWebsite/SeoModule/footer.html.twig" %}
{% endblock %}

