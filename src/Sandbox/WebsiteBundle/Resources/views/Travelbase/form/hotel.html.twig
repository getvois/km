<script>
    $(document).ready(function () {
        $('#booking-form').submit(function () {

            if($('#booking-frame').length == 0){
                $(this).attr('action', '{{ formUrl }}');
                $(this).attr('target', '_self');
            }

            return true;
        });
    });
</script>