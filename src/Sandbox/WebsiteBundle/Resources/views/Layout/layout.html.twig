<div class="row">
    <div class="col-md-9 col-md-offset-0_5">
		
        <!-- Travelbase -->
        {% include '@SandboxWebsite/Travelbase/tabs.html.twig' %}

        {% for flashMessage in app.session.flashbag.get('info') %}

            <div class="alert alert-info">{{ flashMessage }}</div>

        {% endfor %}

        <section class="">
            <div class="row">
                <div class="col-md-6 col--padded">
                    {% block breadcrumbs %}
                        {% if nodemenu is defined and nodemenu.breadCrumb|length > 1 %}
                            <nav role="navigation">
                                <ol class="breadcrumb">
                                    <li>
                                        {% set homepagePageNode = nodemenu.getNodeByInternalName('homepage') %}
                                        <a href="{{ path('_slug', { 'url': homepagePageNode.slug }) }}" class="breadcrumb__home-link">
                                            <i class="icon-home"></i>
                                        </a>
                                    </li>
                                    {% for node in nodemenu.breadCrumb %}
                                        {% if not loop.first %}
                                            {% if loop.last %}
                                                <li class="active">{{ node.title }}</li>
                                            {% else %}
                                                <li>
                                                    <a href="{{ path('_slug', { 'url': node.slug }) }}">{{ node.title }}</a>
                                                </li>
                                            {% endif %}
                                        {% endif %}
                                    {% endfor %}
                                </ol>
                            </nav>
                        {% endif %}
                    {% endblock breadcrumbs %}
                </div>
            </div>
        </section>
		
		 <!-- Main -->
	        <main role="main">
	            {% block content %}{% endblock %}
	        </main>
    </div>
    <div class="col-md-3 sidebar">

        {% block top10 %}
            {{ render(controller('SandboxWebsiteBundle:Travelbase:topTen')) }}
        {% endblock %}

        {% block sidebar %}

        {% endblock %}
		
    </div>
</div>


{% block footer %}
    <!-- Footer -->
    <footer role="footer" class="footer">
        <section class="splash splash--gray-dark">
            <div class="container"> 
                {% if nodemenu is defined %}
                    {% set searchPageNode = nodemenu.getNodeByInternalName('search') %}
                    {% if searchPageNode %}
                        <div class="row row--padded hidden-md hidden-lg">
                            <div class="col-xs-12 col-sm-5 col-sm-offset-7">
                                <form method="get" action="{{ path('_slug', { 'url': searchPageNode.slug }) }}" class="searchbox">
                                    <div class="searchbox-box">
                                        <input type="text" name="query" id="query" value="{% if q_query is defined and q_query != '' %}{{ q_query }}{% endif %}" placeholder="search" class="searchbox__input" />
                                        <button type="submit" id="search" class="searchbox__submit">
                                            <i class="icon-search searchbox__submit__icon"></i>
                                        </button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    {% endif %}

                    {% set activeSubnode = nodemenu.getActiveForDepth(1) %}
                    <div class="row row--padded">
                        {% for node in activeSubnode.children %}
                            {% if not node.node.isHiddenFromNav() and ( node.node.refEntityName == "Sandbox\\WebsiteBundle\\Entity\\Article\\ArticleOverviewPage" or node.node.refEntityName == "Sandbox\\WebsiteBundle\\Entity\\Pages\\ContentPage")  %}
                                <nav class="col-xs-12 col-sm-3 col-md-3 col--padded doormat">
                                    <h5 class="doormat__title"><a href="{{ path('_slug', { 'url': node.slug }) }}">{{ node.title }}</a></h5>
                                    <ul class="list-reset doormat__link-list">
                                        {% if node.children|length>0 %}
                                            {% if node.node.internalName == 'article_overview_page' %}
                                                {% for node2 in node.children %}
                                                    {% if loop.index < 4 %}
                                                        {% if node2.title|length > 40 %}
                                                            {% set ltitle = node2.title[:40] ~ '...' %}
                                                        {% else %}
                                                            {% set ltitle = node2.title %}
                                                        {% endif %}
                                                        <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                            <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ ltitle }}</a>
                                                        </li>
                                                    {% endif %}
                                                {% endfor %}
                                            {% else %}
                                                {% for node2 in node.children %}
                                                    {% if not node.node.isHiddenFromNav() and loop.index < 10%}
                                                        {# check if host exists#}
                                                        {% if host %}
                                                            {% if(node2.page.hosts) is defined %}
                                                                {# output only host based content #}
                                                                {% if host in node2.page.hosts %}
                                                                    <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                        <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                                    </li>
                                                                {% endif %}
                                                            {% else %}
                                                                {# object without hosts. print as normal#}
                                                                <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                    <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                                </li>
                                                            {% endif %}
                                                        {% else %}
                                                            <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                            </li>
                                                        {% endif %}
                                                    {% endif %}
                                                {% endfor %}
                                            {% endif %}
                                        {% endif %}
                                    </ul>
                                </nav>
                            {% endif %}
                        {% endfor %}


                        <nav class="col-xs-12 col-sm-6 col-md-6 col--padded doormat">
                            <img src="{{ asset("bundles/sandboxwebsite/img/general/world_map.png") }}" alt="World map" class="img-reponsive"/>

                            <div class="row">

                                {% for node in activeSubnode.children %}
                                    {% if not node.node.isHiddenFromNav() and ( node.node.refEntityName == "Sandbox\\WebsiteBundle\\Entity\\Place\\PlaceOverviewPage")  %}
                                        <nav class="col-xs-12 col--padded doormat">
                                            <h5 class="doormat__title"><a href="{{ path('_slug', { 'url': node.slug }) }}">{{ node.title }}</a></h5>

                                            <div class="row">

                                                {% set total = 0 %}

                                                {% if node.children|length>0 %}
                                                    {% for node2 in node.children %}
                                                        {% if not node.node.isHiddenFromNav() %}
                                                            {# check if host exists#}
                                                            {% if host %}
                                                                {% if(node2.page.hosts) is defined %}
                                                                    {# output only host based content #}
                                                                    {% if host in node2.page.hosts %}
                                                                        {% set total = total + 1 %}
                                                                    {% else %}
                                                                        {#{% set total = total - 1 %}#}
                                                                    {% endif %}
                                                                {% else %}
                                                                    {# object without hosts. print as normal#}
                                                                    {% set total = total + 1 %}
                                                                {% endif %}
                                                            {% else %}
                                                                {% set total = total + 1 %}
                                                            {% endif %}
                                                        {% else %}
                                                            {#{% set total = total - 1 %}#}
                                                        {% endif %}
                                                    {% endfor %}
                                                {% endif %}





                                            {% set braker = total / 4 %}
                                            {% set counter = 0 %}
                                                {% if node.children|length>0 %}
                                                        {% for node2 in node.children %}
                                                            {% if counter == 0 or loop.index == 0 %}
                                                                <ul class="list-reset doormat__link-list col-xs-6 col-sm-3 col-md-3">
                                                            {% endif %}

                                                            {% if not node.node.isHiddenFromNav() %}
                                                                {# check if host exists#}
                                                                {% if host %}
                                                                    {% if(node2.page.hosts) is defined %}
                                                                        {# output only host based content #}
                                                                        {% if host in node2.page.hosts %}
                                                                            <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                                <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                                            </li>
                                                                            {% set counter = counter + 1 %}
                                                                        {% else %}
                                                                            {#{% set counter = counter - 1 %}#}
                                                                        {% endif %}
                                                                    {% else %}
                                                                        {# object without hosts. print as normal#}
                                                                        <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                            <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                                        </li>
                                                                        {% set counter = counter + 1 %}
                                                                    {% endif %}
                                                                {% else %}
                                                                    <li class="doormat__list-item {% if(node2.active) %}active{% endif %}">
                                                                        <a href="{{ path('_slug', { 'url': node2.slug }) }}" class="link--gray doormat__link">{{ node2.title }}</a>
                                                                    </li>
                                                                    {% set counter = counter + 1 %}
                                                                {% endif %}
                                                            {% else %}
                                                                {#{% set counter = counter - 1 %}#}
                                                            {% endif %}


                                                            {% if counter == 0 %}
                                                                {% set counter = counter + 1 %}
                                                            {% endif %}
                                                            {#{% set counter = counter + 1 %}#}
                                                        {% if counter >= braker %}
                                                            </ul>
                                                            {% set counter = 0 %}
                                                        {% else %}

                                                        {% endif %}

                                                        {% endfor %}
                                                {% endif %}
                                            </div>
                                        </nav>
                                    {% endif %}
                                {% endfor %}


                            </div>

                        </nav>


                    </div>
                {% endif %}
            </div>

            {% block seoFooter %}{% endblock %}
        </section>
        <section class="hidden-xs hidden-sm hidden-md hidden-lg">
            <div class="container">
                <div class="row row--padded">
                    <div class="col-md-12">
                        <p class="pull-left">
                            &copy; {{ "now"|date("Y") }}
                            {% if nodemenu is defined %}
                                {% set sitemap = nodemenu.getNodeByInternalName('sitemap') %}
                                {% if (sitemap != null and sitemap.online) %}- <a href="{{ path('_slug', { 'url': sitemap.slug }) }}">{{ sitemap.title }}</a>{% endif %}
                            {% endif %}
                        </p>
                        <p class="pull-right created-by">

                        </p>
                    </div>
                </div>
            </div>
        </section>
    </footer>
{% endblock %}

