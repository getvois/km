{% if get_node_for(place).level > 2 %}
    {% set type = 'city' %}
{% else %}
    {% set type = 'country' %}
{% endif %}

<a data-id="{{ place.cityId }}" data-type="{{ type }}" data-lvl="{{ get_node_for(place).level }}" href="{{ path("_slug", { 'url' : get_node_translation_for(place).fullSlug }) }}">{{ place.title }}</a>